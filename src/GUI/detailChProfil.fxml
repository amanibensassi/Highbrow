<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="326.0" prefWidth="221.0" spacing="10.0" stylesheets="@../styles/profilCh.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.DetailChProfilController">
   <Label contentDisplay="CENTER" prefHeight="20.0" prefWidth="140.0" text=" Votre Chauffeur" />
    <ImageView id="profileImage" fx:id="profileImage" fitHeight="145.0" fitWidth="154.0" preserveRatio="true" styleClass="profileImage">
      </ImageView>
    <HBox spacing="10.0">
        <Label text="Name:" />
        <Label fx:id="nom" />
    </HBox>
    <HBox spacing="10.0">
        <Label text="prenom" />
        <Label fx:id="prenom" />
    </HBox>
    <HBox spacing="10.0">
        <Label text="Email:" />
        <Label fx:id="email" />
    </HBox>
   <HBox spacing="10.0">
      <children>
         <Label text="num_tel:" />
         <Label fx:id="numtel" />
      </children>
   </HBox>
</VBox>
